programa {
  funcao inicio() 
  {
    real N1, N2, N3, N4, MA
  escreva("Entre com as quatro notas: \n")
  leia(N1, N2, N3, N4) 
  MA = (N1 + N2 + N3 + N4) /4
  escreva("A média é ", MA, "\n")

  se (MA >= 7)
  {
    escreva("Aluno Aprovado! \n")
  } 
  senao {escreva("Reprovadinho !")}
 
    
  }
}
